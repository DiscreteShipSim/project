function [pns] =ship_sim_pdf()

pns.PN_name = 'Ship Simulation for ports';

pns.set_of_Ps = {'p_Arr_Port', 'p_DockArr','p_sadmin','p_Docked', 'p_dadmin','p_ready_to_leave',...
    'p_DockDep','p_finishLoad','p_finishUnload','p_Departed'};
pns.set_of_Ts = {'t_arr','t_wait','t_DArr1','t_DArr2','t_DArr3','t_load','t_unload','t_request_to_leave','t_depart','t_Ddep1','t_Ddep2','t_Ddep3'};


pns.set_of_As = {'t_arr','p_Arr_Port',1, 'p_Arr_Port','t_wait',1, ...
't_wait','p_DockArr',1,'p_DockArr','t_DArr1',1, 'p_DockArr','t_DArr2',1,...
'p_DockArr','t_DArr3',1, ...
't_DArr1','p_Docked',1,'t_DArr2','p_Docked',1, ...
't_DArr3','p_Docked',1, 'p_Docked','t_load',1, ...
'p_Docked','t_unload',1,'t_unload','p_finishUnload',1, ...
'p_finishUnload','t_load',1, 't_load','p_finishLoad',1, ...
'p_finishLoad','t_request_to_leave',1, 't_request_to_leave','p_ready_to_leave',1, ...
'p_ready_to_leave','t_depart',1,'t_depart','DockDep',1, 'DockDep','Ddep1',1, ...
'DockDep','Ddep2',1, 'DockDep','Ddep3',1, 'Ddep1','Departed',1,'Ddep2','Departed',1, ...
'Ddep3','Departed',1
};